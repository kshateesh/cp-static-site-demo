{
  "redirects": [
    {
      "source": "/posts/:path",
      "destination": "/news/:path"
    }
  ],
    "rewrites": [
      {
        "source": "/posts/:path",
        "destination": "https://jsonplaceholder.typicode.com/posts/:path"
      },
            {
                "source": "/path{2,3}",
                "destination": "/blog.html/*"
            }
        
    
    ]
}
